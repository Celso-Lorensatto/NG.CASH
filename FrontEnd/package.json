{
  "name": "ng.cash_webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "watch:sass": "node-sass src/styles/sass/main.scss src/styles/global.css -w",
    "compile:sass": "node-sass src/styles/sass/main.scss src/styles/css/global.comp.css",
    "concat:css": "concat -o src/styles/css/global.concat.css src/styles/css/global.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' src/styles/css/global.concat.css -o src/styles/css/global.prefix.css",
    "compress:css": "node-sass src/styles/css/global.prefix.css src/styles/global.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.10",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "axios": "^1.1.3",
    "next": "13.0.4",
    "nookies": "^2.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-input-mask": "^2.0.4",
    "react-query": "^3.39.2",
    "typescript": "4.9.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/nookies": "^2.0.3",
    "@types/react-input-mask": "^3.0.2",
    "@types/react-query": "^1.2.9",
    "@types/yup": "^0.32.0",
    "autoprefixer": "^10.4.13",
    "concat": "^1.0.3",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.0.0"
  }
}
